<UserControl
    x:Class="YimInjectorAlt.Views.HacksView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://crazyzhang.cn/ui/2023/xaml"
    d:DesignHeight="211"
    Style="{StaticResource UserControlStyle_Loaded}"
    mc:Ignorable="d" Width="652">

    <ScrollViewer>
        <WrapPanel>
            <!--  YimMenu 注入式菜单  -->
            <ui:GroupBoxHack
                Title="YimMenu 汉化测试"
                Description="测试版-仅用于测试Yimmenu汉化，不要使用其他功能，可以与原版小助手同时运行"
                Image="" Height="211" Width="640">
                <ui:GroupBoxHack.TitleContent>
                    <StackPanel Orientation="Horizontal" Width="90">
                        <Button x:Name="text10"
                            Command="{Binding CheatsClickCommand}"
                            CommandParameter="YimMenu"
                            Content="注入DLL" Width="72" />
                    </StackPanel>
                </ui:GroupBoxHack.TitleContent>
            </ui:GroupBoxHack>

        </WrapPanel>
    </ScrollViewer>

</UserControl>
